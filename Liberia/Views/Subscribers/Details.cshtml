@model SubscriberVM

@{
    ViewData["Title"] = "Subscriber Details";
}

<div class="d-flex flex-column flex-xl-row">
    <!--begin::Sidebar-->
    <div class="flex-column flex-lg-row-auto w-100 w-xl-350px mb-10">
        <!--begin::Card-->
        <div class="card mb-5 mb-xl-8">
            <!--begin::Card body-->
            <div class="card-body pt-15">
                <!--begin::Summary-->
                <div class="d-flex flex-center flex-column mb-5">
                    <!--begin::Avatar-->
                    <div class="symbol symbol-100px symbol-circle mb-7">
                        <img src="@Model.ProfilePhoto" alt="image">
                    </div>
                    <!--end::Avatar-->
                    <!--begin::Name-->
                    <a class="fs-3 text-gray-800 text-hover-primary fw-bolder mb-1">@Model.FirstName @Model.LastName</a>
                    <!--end::Name-->
                    <!--begin::Position-->
                    <div class="fs-7 mt-2 badge badge-light-@(Model.IsBlocked ? "danger":"success") d-inline">@(Model.IsBlocked ? "Black listed Subscriber" : "Active Subscriber")</div>
                    @*<div class="fs-5 fw-bold text-muted mb-6">Subscriber</div>*@
                    <!--end::Position-->
                </div>
                <!--end::Summary-->
                <!--begin::Details toggle-->
                <div class="d-flex flex-stack fs-4 py-3">
                    <div class="fw-bolder rotate collapsible" data-bs-toggle="collapse" href="#kt_customer_view_details" role="button" aria-expanded="false" aria-controls="kt_customer_view_details">
                        Details
                        <span class="ms-2 rotate-180">
                            <!--begin::Svg Icon | path: icons/duotune/arrows/arr072.svg-->
                            <span class="svg-icon svg-icon-3">
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                    <path d="M11.4343 12.7344L7.25 8.55005C6.83579 8.13583 6.16421 8.13584 5.75 8.55005C5.33579 8.96426 5.33579 9.63583 5.75 10.05L11.2929 15.5929C11.6834 15.9835 12.3166 15.9835 12.7071 15.5929L18.25 10.05C18.6642 9.63584 18.6642 8.96426 18.25 8.55005C17.8358 8.13584 17.1642 8.13584 16.75 8.55005L12.5657 12.7344C12.2533 13.0468 11.7467 13.0468 11.4343 12.7344Z" fill="black"></path>
                                </svg>
                            </span>
                            <!--end::Svg Icon-->
                        </span>
                    </div>
                    <span data-bs-toggle="tooltip" data-bs-trigger="hover" title="" data-bs-original-title="Edit customer details">
                        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-sm btn-light-primary">Edit</a>
                    </span>
                </div>
                <!--end::Details toggle-->
                <div class="separator separator-dashed my-3"></div>
                <!--begin::Details content-->
                <div id="kt_customer_view_details" class="collapse show">
                    <div class="py-4 fs-6">
                        <!--begin::Details item-->
                        <div class="fw-bolder">National ID</div>
                        <div class="text-gray-600">@Model.NationalId</div>
                        <!--begin::Details item-->
                        <!--begin::Details item-->
                        <div class="fw-bolder mt-5">Email</div>
                        <div class="text-gray-600">
                            <a class="text-gray-600 text-hover-primary">@Model.Email</a>
                        </div>
                        <!--begin::Details item-->
                        <div class="fw-bolder mt-5">Mobile Number</div>
                        <div class="text-gray-600">@Model.PhoneNumber</div>
                        <!--begin::Details item-->
                        <div class="fw-bolder mt-5">Address</div>
                        <div class="text-gray-600">
                            @Model.Address
                            <br>PostalCode: @Model.ZipCode
                            <br>@Model.Area ,@Model.Governorate
                        </div>
                        <!--begin::Details item-->
                        <!--begin::Details item-->
                        <div class="fw-bolder mt-5">Joined On</div>
                        <div class="text-gray-600">@Model.JoinedOn</div>
                        <!--begin::Details item-->
                    </div>
                </div>
                <!--end::Details content-->
            </div>
            <!--end::Card body-->
